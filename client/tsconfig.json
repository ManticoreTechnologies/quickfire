{
  "compilerOptions": {
    "target": "esnext",
    "module": "Node16",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*", "client/**/*"],
  "exclude": ["node_modules"]
}